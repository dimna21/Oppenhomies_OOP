<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>ServletPackage.ListenerClass</listener-class>
  </listener>

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>ServletPackage.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>NewAccServlet</servlet-name>
    <servlet-class>ServletPackage.NewAccServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>ServletPackage.LogoutServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LookUpServlet</servlet-name>
    <servlet-class>ServletPackage.LookUpServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddFriendServlet</servlet-name>
    <servlet-class>ServletPackage.AddFriendServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AcceptFriendRequestServlet</servlet-name>
    <servlet-class>ServletPackage.AcceptFriendRequestServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>RejectFriendRequestServlet</servlet-name>
    <servlet-class>ServletPackage.RejectFriendRequestServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AcceptChallengeServlet</servlet-name>
    <servlet-class>ServletPackage.AcceptChallengeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>RejectChallengeServlet</servlet-name>
    <servlet-class>ServletPackage.RejectChallengeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateProfilePictureServlet</servlet-name>
    <servlet-class>ServletPackage.UpdateProfilePictureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NewAccServlet</servlet-name>
    <url-pattern>/NewAccServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LookUpServlet</servlet-name>
    <url-pattern>/LookUpServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddFriendServlet</servlet-name>
    <url-pattern>/AddFriendServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AcceptFriendRequestServlet</servlet-name>
    <url-pattern>/AcceptFriendRequestServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RejectFriendRequestServlet</servlet-name>
    <url-pattern>/RejectFriendRequestServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AcceptChallengeServlet</servlet-name>
    <url-pattern>/AcceptChallengeServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RejectChallengeServlet</servlet-name>
    <url-pattern>/RejectChallengeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateProfilePictureServlet</servlet-name>
    <url-pattern>/UpdateProfilePictureServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Homepage/Wellcome.jsp</welcome-file>
  </welcome-file-list>
</web-app>
